<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="my_6dof_arm">

  <!-- Macro to create a simple box link -->
  <xacro:macro name="simple_link" params="name xyz rpy size color">
    <link name="${name}">
      <visual>
        <origin xyz="${xyz}" rpy="${rpy}"/>
        <geometry>
          <box size="${size}"/>
        </geometry>
        <material name="${color}"/>
      </visual>
      <collision>
        <origin xyz="${xyz}" rpy="${rpy}"/>
        <geometry>
          <box size="${size}"/>
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="1.0"/>
        <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
      </inertial>
    </link>
  </xacro:macro>

  <!-- Links -->
  <xacro:simple_link name="base_link" xyz="0 0 0.05" rpy="0 0 0" size="0.1 0.1 0.1" color="Grey"/>
  <xacro:simple_link name="link_1" xyz="0 0 0.1" rpy="0 0 0" size="0.05 0.05 0.2" color="Blue"/>
  <xacro:simple_link name="link_2" xyz="0 0 0.2" rpy="0 0 0" size="0.05 0.05 0.2" color="Green"/>
  <xacro:simple_link name="link_3" xyz="0 0 0.2" rpy="0 0 0" size="0.05 0.05 0.2" color="Red"/>
  <xacro:simple_link name="link_4" xyz="0 0 0.15" rpy="0 0 0" size="0.04 0.04 0.15" color="Yellow"/>
  <xacro:simple_link name="link_5" xyz="0 0 0.1" rpy="0 0 0" size="0.03 0.03 0.1" color="Cyan"/>
  <xacro:simple_link name="link_6" xyz="0 0 0.1" rpy="0 0 0" size="0.02 0.02 0.1" color="Magenta"/>
  <xacro:simple_link name="ee_link" xyz="0 0 0.05" rpy="0 0 0" size="0.05 0.05 0.05" color="White"/>

  <!-- Joints (same as before) -->
  <joint name="joint_1" type="revolute">
    <parent link="base_link"/>
    <child link="link_1"/>
    <origin xyz="0 0 0.1" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14" upper="3.14" effort="50" velocity="1.0"/>
  </joint>

  <joint name="joint_2" type="revolute">
    <parent link="link_1"/>
    <child link="link_2"/>
    <origin xyz="0 0 0.2" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14" upper="3.14" effort="50" velocity="1.0"/>
  </joint>

  <joint name="joint_3" type="revolute">
    <parent link="link_2"/>
    <child link="link_3"/>
    <origin xyz="0 0 0.2" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14" upper="3.14" effort="50" velocity="1.0"/>
  </joint>

  <joint name="joint_4" type="revolute">
    <parent link="link_3"/>
    <child link="link_4"/>
    <origin xyz="0 0 0.15" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-3.14" upper="3.14" effort="50" velocity="1.0"/>
  </joint>

  <joint name="joint_5" type="revolute">
    <parent link="link_4"/>
    <child link="link_5"/>
    <origin xyz="0 0 0.1" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14" upper="3.14" effort="50" velocity="1.0"/>
  </joint>

  <joint name="joint_6" type="revolute">
    <parent link="link_5"/>
    <child link="link_6"/>
    <origin xyz="0 0 0.1" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-3.14" upper="3.14" effort="50" velocity="1.0"/>
  </joint>

  <joint name="ee_joint" type="fixed">
    <parent link="link_6"/>
    <child link="ee_link"/>
    <origin xyz="0 0 0.05" rpy="0 0 0"/>
  </joint>

  <!-- ros2_control transmissions (same as before) -->
  <transmission name="joint_1_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_1"/>
    <actuator name="joint_1_motor"/>
  </transmission>

  <transmission name="joint_2_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_2"/>
    <actuator name="joint_2_motor"/>
  </transmission>

  <transmission name="joint_3_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_3"/>
    <actuator name="joint_3_motor"/>
  </transmission>

  <transmission name="joint_4_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_4"/>
    <actuator name="joint_4_motor"/>
  </transmission>

  <transmission name="joint_5_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_5"/>
    <actuator name="joint_5_motor"/>
  </transmission>

  <transmission name="joint_6_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_6"/>
    <actuator name="joint_6_motor"/>
  </transmission>

</robot> 