            <origin xyz="0.0 0.0 0.340" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.05" length="0.680"/>
            </geometry>
            <material name="Red"/>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.340" rpy="0.0 0.0 0.0"/>
