    <joint name="link1_link2" type="revolute">
        <parent link="link_1"/>
        <child link="link_2"/>
        <origin xyz="0.0 0.1 0.214" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="-2.50" upper="2.50" effort="0.5" velocity="1000.0"/>    
    </joint>

    <link name="link_3">
