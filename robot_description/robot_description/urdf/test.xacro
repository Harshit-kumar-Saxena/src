    <joint name="link3_link4" type="revolute">
        <parent link="link_3"/>
        <child link="link_4"/>
        <origin xyz="0.0 0.1 0.630" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="-3.14" upper="3.14" effort="0.5" velocity="1000.0"/>
    </joint>

    <link name="link_5">
        <visual>
            <origin xyz="0.0 0.0 0.035" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.05" length="0.170"/>
            </geometry>
            <material name="Cyan"/>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.035" rpy="0.0 0.0 0.0"/>
