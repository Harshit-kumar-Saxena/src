    </link>

    <joint name="link2_link3" type="revolute">
        <parent link="link_2"/>
        <child link="link_3"/>
        <origin xyz="0.0 -0.1 0.8" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="-3.14" upper="3.14" effort="0.5" velocity="1000.0"/>
    </joint>

    <link name="link_4">
        <visual>
            <origin xyz="0.0 0.0 0.065" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.05" length="0.130"/>
            </geometry>
            <material name="Yellow"/>
