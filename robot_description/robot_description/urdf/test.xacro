<?xml version="1.0"?>
<robot name="robot_6dof">

    <!-- Links -->
    <link name="base_link">
        <visual>
            <origin xyz="0.0 0.0 0.025" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
            <material name="Grey"/>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.025" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
        </inertial>
    </link>

    <link name="link_1">
