    <joint name="link5_tcp_connector" type="revolute">
        <parent link="link_5"/>
        <child link="tcp_connector"/>
        <origin xyz="0.0 0.0 0.120" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-3.14" upper="3.14" effort="0.5" velocity="1000.0"/>
    </joint>

    <link name="gripper_right">
        <visual>
            <origin xyz="0.0 0.0 0.05" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.01 0.01 0.1"/>
            </geometry>
            <material name="Black"/>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.05" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.01 0.01 0.1"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
